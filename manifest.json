{
  "manifest_version": 2,
  "name": "Disctraction Blocker",
  "description": "Eliminate distractions for fullstackers.",
  "version": "1.0",
  "author": "Jimmi Chen",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Switch Mode"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "storage"
  ],
  "homepage_url": "https://learn.fullstackacademy.com/",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}