{
  "manifest_version": 2,
  "name": "Disctraction Blocker",
  "description": "Eliminate distractions for fullstackers.",
  "version": "1.0",
  "author": "Jimmi Chen",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click for settings"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "homepage_url": "https://learn.fullstackacademy.com/",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}